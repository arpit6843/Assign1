import java.io.*;
public class sum {
    public static void main(String[] args) {
        sum obj=new sum();
        obj.returnNumber();
   }
    public int returnNumber() {
        int c=0;
        float a = 7.8f;
        float b = 4.4f;
        c=(int)a+(int)b;
        System.out.println("Value of C is: "+c);
        return c;
}}
