//Java Program to Count Number of Objects Created for Class.

import java.io.*;
public class Count_objects {
    static int c=0;
    Count_objects()
    {
        c++;
    }
    
    public static void main(String[] args)
    {
        Count_objects a1= new Count_objects();
        Count_objects a2= new Count_objects();
        Count_objects a3= new Count_objects();
        Count_objects a4= new Count_objects();
        Count_objects a5= new Count_objects();
        
        System.out.println("No of objects created "+c);
   }
}
