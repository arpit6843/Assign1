//Write a java program to add two float numbers like 8.6 and 9.7.

import java.io.*;
public class Add 
{
    public static void main (String[] args)
    {
        float a=8.6f;
        float b=9.7f;
        float sum=a+b;
     
        System.out.println("Enter no 1->"+a);
        System.out.println("Enter no 2->"+b);
        System.out.println("Addition of 2 nos"+sum);
    }
}
